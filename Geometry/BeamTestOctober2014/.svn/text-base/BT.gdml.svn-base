 <define>   
 </define>

 <materials>

  <!--Vacuum-->
  <element name="videRef" formula="VACUUM" Z="1">
   <atom value="1."/>
  </element>
  <material formula=" " name="Vacuum">
   <D value="1.e-25" unit="g/cm3" />
   <fraction n="1.0" ref="videRef" />
  </material>

 </materials>


 <solids> 
  <box lunit="mm" name="beam_test_world_box"   x="18000" y="18000" z="18000" /> 
 </solids>


 <structure>
  
  <volume name="beam_test_auxiliary" >
   <materialref ref="Vacuum" />
   <solidref ref="beam_test_world_box" />     
    
    <physvol>
      <file name="./Geometry/BeamTestOctoberAux/LDR0_Ascii.gdml"/> 
    </physvol>

    <physvol>
      <file name="./Geometry/BeamTestOctoberAux/NaI_Ascii.gdml"/> 
    </physvol>

    <physvol>
      <file name="./Geometry/BeamTestOctoberAux/S0_Ascii.gdml"/> 
    </physvol>

    <physvol>
      <file name="./Geometry/BeamTestOctoberAux/S1_Ascii.gdml"/> 
    </physvol>

    <physvol>
      <file name="./Geometry/BeamTestOctoberAux/S2_Ascii.gdml"/> 
    </physvol>

    <physvol>
      <file name="./Geometry/BeamTestOctoberAux/S3_Ascii.gdml"/> 
    </physvol>

    <physvol>
      <file name="./Geometry/BeamTestOctoberAux/Sh_Ascii.gdml"/> 
    </physvol>

    <physvol>
      <file name="./Geometry/BeamTestOctoberAux/SSD0_Ascii.gdml"/> 
    </physvol>

    <physvol>
      <file name="./Geometry/BeamTestOctoberAux/SSD1_Ascii.gdml"/> 
    </physvol>
   
        
  </volume>
 </structure>


 <setup name="Default" version="1.0" >
  <world ref="beam_test_auxiliary" />
 </setup>


</gdml>
